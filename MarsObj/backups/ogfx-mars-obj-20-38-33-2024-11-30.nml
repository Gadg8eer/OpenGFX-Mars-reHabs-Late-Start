/* Templates. */
template template_object(n) {
  // left top width height x y
  [4 + 64 * n + 4 * n, 4, 64, 64, -31, -32]
}
template template_tall_object(n) {
  // left top width height x y
  [4 + 72 * n + 4 * n, 4, 72, 128, -35, -96]
}
