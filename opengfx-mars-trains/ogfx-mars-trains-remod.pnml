// Define the NewGRF
grf {
	grfid: "RJE\12";
	name: string(STR_GRF_NAME);
	desc: string(STR_GRF_DESCRIPTION);
	url:  string(STR_GRF_URL);
	version: 1;
	min_compatible_version: 1;
}

#include "../graphics/common_nml/cargo_table.pnml"


//Define the rail type translation table
railtypetable {
	MONO, //Monorail
	MGLV  //Maglev
}

//Disable all default vehicles
disable_item(FEAT_TRAINS, 0, 115); 

// Define the Monorail templates
//Based on Pikka's alignment templates: http://users.tt-forums.net/pikka/wiki/index.php?title=Sprite_templates&oldid=3941
//Assumes a PixelTool generated set of sprites
template template_monotr_8PT(x, y) {
	[0, 0, 8, 26, -3, -17]
	[9, 0, 22, 21, -11, -14]
	[32, 0, 32, 13, -16, -8]
	[65, 0, 22, 21, -11, -14]
	[88, 0, 8, 26, -3, -17]
	[97, 0, 22, 21, -11, -14]
	[120, 0, 32, 13, -16, -8]
	[153, 0, 22, 21, -11, -14]
}

