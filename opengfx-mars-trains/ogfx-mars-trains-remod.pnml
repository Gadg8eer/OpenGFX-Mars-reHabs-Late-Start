item(FEAT_TRAINS, spriteset_monorail_carriage_pax) {
	property {
		//Basic properties
		name:				string(STR_NAME_MONORAIL_CARRIAGE_PAX);
		climates_available:		bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL, CLIMATE_TOYLAND);

		//Ground train general properties
		track_type:			MONO;

		//Vehicle properties
		introduction_date:		date(2032, 01, 01);
		model_life:				VEHICLE_NEVER_EXPIRES;
		vehicle_life:			35;
		//Zero power, so carriage
		power:				0;
		length:				8;
		weight:				14;

		//Cargo properties
		cargo_capacity:			28;
		default_cargo_type:		DEFAULT_CARGO_FIRST_REFITTABLE;
		refittable_cargo_classes:	bitmask(CC_PASSENGERS);
		//non_refittable_cargo_classes:	not defined, so none excluded
		cargo_allow_refit:		[PASS]; //Fallback to guarantee fitting
		//cargo_disallow_refit:		not defined, so none excluded
		loading_speed:			3; //NB. deliberately low for pressurised cargo

		//Costs
		cost_factor:			12; //NB. deliberately high for pressurised cargo
		//Carriage, so no running cost defined

		//Graphics
		misc_flags:			bitmask(TRAIN_FLAG_2CC);
		sprite_id:			SPRITE_ID_NEW_TRAIN;
	}

	graphics {
		default: spriteset_monorail_carriagepax;
	}
}

